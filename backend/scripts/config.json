{
  "geojson_processing": {
    "default_sample_size": 50,
    "max_sample_size": 1000,
    "supported_formats": [".geojson", ".json"],
    "output_format": "enviducate_mockdata",
    "gemini_enabled": true,
    "fallback_analysis": true
  },
  "categories": {
    "energy": {
      "keywords": ["gas", "energy", "oil", "fuel", "power", "electricity"],
      "charities": [
        {
          "name": "Michigan Environmental Council",
          "url": "https://www.environmentalcouncil.org",
          "description": "Advocating for clean energy in Michigan"
        },
        {
          "name": "Sierra Club Michigan",
          "url": "https://www.sierraclub.org/michigan",
          "description": "Environmental protection and clean energy advocacy"
        }
      ]
    },
    "water": {
