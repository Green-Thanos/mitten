        {
          "name": "Michigan Nature Association",
          "url": "https://www.michigannature.org",
          "description": "Wildlife habitat protection"
        }
      ]
    },
    "air": {
      "keywords": ["air", "pollution", "quality", "emission", "aerosol", "atmosphere"],
